{
  "C_Cpp.intelliSenseEngine": "disabled",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.tabSize": 4
  },
  "cSpell.userWords": [
    "Arjan",
    "cdap",
    "centigrate",
    "Deque",
    "Gson",
    "guice",
    "Manacher's",
    "manachers",
    "neighbour",
    "neighbours",
    "peekable",
    "stackframe",
    "tolerations"
  ],
  "git.suggestSmartCommit": false,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": true,
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": true,
  "[python]": {
    "gitlens.codeLens.symbolScopes": ["!Module"],
    "editor.wordBasedSuggestions": "off",
    "editor.formatOnType": true
  },
  "python.linting.pycodestyleEnabled": true,
  "python.linting.pycodestyleArgs": ["--ignore=E501"],
  "python.linting.pylintArgs": ["-d", "C0301"],
  "java.completion.guessMethodArguments": true,
  "java.signatureHelp.enabled": true,
  "java.signatureHelp.description.enabled": true,
  "java.saveActions.organizeImports": true,
  "java.referencesCodeLens.enabled": true,
  "java.maxConcurrentBuilds": 3,
  "java.maven.downloadSources": true,
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSaveMode": "modifications",
  // "editor.formatOnSave": true,
  "Lua.telemetry.enable": false,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.new"]
    },
    // Vim style buffer navigation.
    {
      "before": ["L"],
      "commands": [":bnext"]
    },
    {
      "before": ["H"],
      "commands": [":bprev"]
    },
    // Open file manager, aka project view.
    {
      "before": ["<leader>", "p", "v"],
      "commands": ["workbench.action.files.openFileFolder"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["editor.action.goToImplementation"]
    },
    // Handle default register overwrite behaviour.
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["c"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["x"],
      "after": ["\"", "_", "x"]
    },
    {
      "before": ["s"],
      "after": ["\"", "_", "s"]
    },
    // Center cursor after moving up/down.
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    // Center search results.
    // This handles "folds".
    {
      "before": ["n"],
      "after": ["n", "z", "z", "z", "v"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z", "z", "v"]
    },
    // Select all.
    {
      "before": ["<C-a>"],
      "after": ["g", "g", "V", "G"]
    },
    // go to problems panel.
    {
      "before": ["/"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "g", "p"],
      "commands": ["workbench.actions.view.problems"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      // Delete without copying to register.
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    // Always change without copying to register.
    {
      "before": ["c"],
      "after": ["\"", "_", "c"]
    },
    // Always remove char without copying to register.
    {
      "before": ["x"],
      "after": ["\"", "_", "x"]
    },
    {
      "before": ["s"],
      "after": ["\"", "_", "s"]
    }
  ],
  "vim.handleKeys": {
    // Allow vs code to handle tab navigation.
    "<C-tab>": false,
    "<C-S-tab>": false,
    // Allow usual copy and paste.
    "<C-c>": false,
    "<C-v>": false,
    "<C-x>": false,
    // vs code new window.
    "<C-n>": false,
    // vs code indention.
    "<C-[>": false,
    "<C-]>": false,
    // vs code tab close.
    "<C-w>": false,
    "<C-f>": false,
    "<C-p>": false
  },
  "vim.overrideCopy": false,
  "vim.useSystemClipboard": true,
  "workbench.sideBar.location": "right",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.lineNumbers": "relative",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "files.trimTrailingWhitespace": true,
  "editor.detectIndentation": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.inferGopath": true,
  "editor.fontLigatures": true,
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "vscode-icons",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "diffEditor.ignoreTrimWhitespace": false
  },
  "nxConsole.showNodeVersionOnStartup": false
}
